{
  "name": "academy-dating-sim",
  "private": true,
  "version": "1.6.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev --workspace=@academy-dating-sim/frontend",
    "build": "npm run build --workspace=@academy-dating-sim/frontend",
    "lint": "npm run lint --workspace=@academy-dating-sim/frontend && npm run lint --workspace=@academy-dating-sim/backend",
    "preview": "npm run preview --workspace=@academy-dating-sim/frontend",
    "deploy": "npm run build && npx gh-pages -d apps/frontend/dist",
    "backend:dev": "npm run dev --workspace=@academy-dating-sim/backend",
    "backend:build": "npm run build --workspace=@academy-dating-sim/backend",
    "mobile:android": "npm run android --workspace=@academy-dating-sim/mobile",
    "mobile:ios": "npm run ios --workspace=@academy-dating-sim/mobile",
    "mobile:sync": "npm run sync --workspace=@academy-dating-sim/mobile",
    "test": "echo \"Add your test command here\""
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
